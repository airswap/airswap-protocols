{
  "name": "@airswap/libraries",
  "version": "4.0.4",
  "description": "AirSwap: Libraries for Developers",
  "repository": {
    "type": "git",
    "url": "https://github.com/airswap/airswap-libraries"
  },
  "homepage": "https://github.com/airswap/airswap-libraries",
  "keywords": [
    "airswap"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "scripts": {
    "clean": "rm -rf ./build",
    "compile": "yarn clean && tsc -b",
    "lint:check": "yarn eslint . --ext .js,.ts",
    "lint:fix": "yarn eslint . --ext .js,.ts --fix",
    "pretty:check": "prettier --check \"./**/*.ts\" \"./**/*.js\"",
    "pretty:fix": "prettier --write \"./**/*.ts\" \"./**/*.js\"",
    "test": "yarn test:unit",
    "test:unit": "TS_NODE_COMPILER_OPTIONS='{\"strict\":false}' yarn mocha -r ts-node/esm test/*.ts"
  },
  "dependencies": {
    "@airswap/constants": "^4.0.0",
    "@airswap/jsonrpc-client-websocket": "0.0.1",
    "@airswap/registry": "4.0.2-beta.0",
    "@airswap/swap-erc20": "^4.0.0",
    "@airswap/types": "^4.0.0",
    "@airswap/utils": "^4.0.0",
    "@airswap/wrapper": "^4.0.0",
    "bignumber.js": "^9.0.0",
    "browser-or-node": "^1.3.0",
    "ethers": "^5.6.9",
    "jayson": "^3.2.0",
    "tiny-typed-emitter": "^2.1.0",
    "websocket": "^1.0.34",
    "ws": "7.5.9"
  },
  "devDependencies": {
    "@relaycorp/ws-mock": "^5.2.5",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "fancy-test": "^1.4.7",
    "mock-require": "^3.0.3",
    "mock-socket": "^9.0.3",
    "nock": "^12.0.1",
    "prettier": "^2.8.4",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.9.0"
  }
}
