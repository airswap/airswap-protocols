{
  "name": "@airswap/wrapper",
  "version": "4.0.3-beta.1",
  "description": "AirSwap: Wrap and Unwrap Native Tokens",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/airswap/airswap-protocols"
  },
  "files": [
    "./build",
    "./typechain",
    "./deploys.js",
    "./deploys-weth.js"
  ],
  "scripts": {
    "clean": "hardhat clean; rm -rf ./build; rm -rf ./typechain",
