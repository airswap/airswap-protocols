  },
  "devDependencies": {
    "@airswap/constants": "4.0.2-beta.1",
    "@airswap/utils": "4.0.5-beta.1",
    "@airswap/types": "4.0.3-beta.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "prompt-confirm": "^2.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
