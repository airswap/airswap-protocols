{
  "name": "@airswap/converter",
  "version": "3.1.2",
  "description": "AirSwap: Protocol Fee Converter",
  "contributors": [
    "Martin Sterlicchi",
    "Hassan Malik",
    "Don Mosites"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/airswap/airswap-protocols"
  },
  "files": [
    "./build/contracts",
    "./typechain/factories",
    "./deploys.js"
  ],
  "scripts": {
    "clean": "hardhat clean; rm -rf ./build; rm -rf ./typechain",
