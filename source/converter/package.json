    "deploy": "hardhat run ./scripts/deploy.js",
    "verify": "hardhat run ./scripts/verify.js"
  },
  "devDependencies": {
    "@uniswap/lib": "1.1.1",
    "@uniswap/v2-core": "1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
