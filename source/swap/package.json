{
  "name": "@airswap/swap",
  "version": "0.0.1",
  "private": true,
  "description": "AirSwap: Atomic Token Swap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/airswap/airswap-protocols"
  },
  "files": [
    "./build",
    "./typechain",
    "./deploys.js"
  ],
  "scripts": {
    "clean": "hardhat clean; rm -rf ./build; rm -rf ./typechain",
    "compile": "hardhat compile; yarn typechain",
